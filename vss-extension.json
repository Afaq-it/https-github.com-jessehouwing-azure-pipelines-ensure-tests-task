{
  "manifestVersion": 1,
  "id": "vsts-ensure-tests-tasks",
  "name": "Ensure Tests",
  "version": "0.0.7",

  "publisher": "jessehouwing",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scope": [
    "vso.build"
  ],
  "description": "Ensure that test results are linked to this build.",
  "categories": [
    "Azure Pipelines", "Azure Test Plans"
  ],
  "public": true,
  "galleryFlags": [
    "Public"
  ],
  "tags": [
    "Task",
    "Pipelines",
    "Extension",
    "Gate",
    "Build",
    "Test",
    "Xpirit"
  ],
  "screenshots": [
    { "path": "extension/Images/Screenshots/configure-phase.png" }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license.txt"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/jessehouwing/vsts-ensure-tests/wiki"
    },
    "support": {
      "uri": "https://github.com/jessehouwing/vsts-ensure-tests/issues"
    }
  },
  "badges": [
    {
      "href": "https://paypal.me/jessehouwing/5",
      "uri": "https://raw.githubusercontent.com/jessehouwing/vsts-ensure-tests/master/extension/images/donate.png",
      "description": "Donate"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/jessehouwing/vsts-ensure-tests"
  },
  "branding": {
    "color": "rgb(36, 43, 50)",
    "theme": "dark"
  },
  "icons": {
    "default": "extension/images/extension-icon.png"
  },
  "files": [
    {
      "path": "EnsureTests"
    },
    { 
      "path": "extension/Images/Screenshots/configure-phase.png",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "vsts-ensure-tests-tasks",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "EnsureTests"
      }
    }
  ]
}
