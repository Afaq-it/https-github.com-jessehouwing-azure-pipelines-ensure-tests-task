{
  "manifestVersion": 1,
  "id": "vsts-ensure-tests-tasks",
  "name": "Ensure Tests",
  "version": "0.0.7",

  "publisher": "jessehouwing",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scope": [
    "vso.build"
  ],
  "description": "Ensure that test results are linked to this build.",
  "categories": [
    "Azure Pipelines", "Azure Test Plans"
  ],
  "public": true,
  "galleryFlags": [
    "Public"
  ],
  "tags": [
    "Task",
    "Pipelines",
    "Extension",
    "Gate",
    "Build",
    "Test",
    "Xpirit"
  ],
  "screenshots": [
    { "path": "extension/Images/Screenshots/configure-phase.png" }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "LICENSE.md"
    },
    "privacy": {
      "path": "PRIVACY.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/jessehouwing/azure-pipelines-ensure-tests-task/wiki"
    },
    "support": {
      "uri": "https://github.com/jessehouwing/azure-pipelines-ensure-tests-task/issues"
    },
    "license": {
      "uri": "https://github.com/jessehouwing/azure-pipelines-ensure-tests-task/LICENSE.md"
    },
    "privacypolicy": {
      "uri": "https://github.com/jessehouwing/azure-pipelines-ensure-tests-task/blob/main/PRIVACY.md"
    }
  },
  "badges": [
    {
      "href": "https://paypal.me/jessehouwing/5",
      "uri": "https://raw.githubusercontent.com/jessehouwing/vsts-ensure-tests/main/extension/images/donate.png",
      "description": "Donate"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/jessehouwing/azure-pipelines-ensure-tests-task"
  },
  "branding": {
    "color": "rgb(36, 43, 50)",
    "theme": "dark"
  },
  "icons": {
    "default": "extension/images/extension-icon.png"
  },
  "files": [
    {
      "path": "EnsureTests"
    },
    {
      "path": "extension/Images/Screenshots/configure-phase.png",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "vsts-ensure-tests-tasks",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "EnsureTests"
      }
    }
  ]
}
